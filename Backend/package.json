    "compression": "^1.7.4",
    "dotenv": "^8.0.0",
    "cookie-parser": "^1.4.4",
    "express": "^4.17.1",
    "method-override": "^3.0.0",
    "socket.io": "^2.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/socket.io": "^2.1.2",
