    "start": "nodemon -r dotenv/config ./dist/index.js",
    "start:staging": "nodemon -r dotenv/config ./dist/index.js dotenv_config_path=.staging.env",
    "start:production": "nodemon -r dotenv/config ./dist/index.js dotenv_config_path=.production.env"
