{
  "compilerOptions": {
      "target": "es6",
      "lib": [
          "es6"
      ],
      "typeRoots": [
          "./node_modules/@types"
      ],
      "outDir": "dist",
      "module": "commonjs",
      "moduleResolution": "node",
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "sourceMap": true,
      "allowSyntheticDefaultImports": true
  },
  "exclude": [
      "node_modules"
  ]
}